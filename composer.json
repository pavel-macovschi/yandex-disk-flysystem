{
    "name": "impressiveweb/yandex-disk-flysystem",
    "description": "Flysystem adapter for Yandex Disk API",
    "homepage": "https://github.com/pavel-macovschi/yandex-disk-flysystem",
    "license": "MIT",
    "minimum-stability": "dev",
    "keywords": [
      "impressiveweb",
      "flysystem",
      "flysystem adapter",
      "filesystem",
      "cloud filesystem",
      "yandex disk flysystem adapter"
    ],
    "authors": [
        {
            "name": "Pavel Macovschi",
            "email": "pavel.macovschi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "impressiveweb/yandex-disk": "^1.0",
        "league/flysystem":"^3.0",
        "php": "^8.1"
   },
    "autoload": {
        "psr-4": {
            "ImpressiveWeb\\Flysystem\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpstan/phpstan": "2.0.x-dev",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "symfony/var-dumper": "7.1.x-dev"
    }
}
